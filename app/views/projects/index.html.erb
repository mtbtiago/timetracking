<h1>Project listing</h1>
<ul>
<% @projects.each do |p| %>
<!-- You don't need to use <a> -->
  <li>
    <% if p.logo_file_name.present? %>
      <%= image_tag p.logo.url(:thumb) %>
    <% end %>
    <%=link_to(p.full_name,project_entries_path(p))%>
    <%= link_to("[edit]",edit_project_path(p), method: :get) %>
    <%= link_to("[delete]",project_path(p), method: :delete, data: { confirm: 'Are you sure?' }) %>
  </li>
<%end%>
</ul>

<h4>
  <div class="new_item button_actions"><%= link_to("New project",new_project_path) %></div>
  <div class="all_items button_actions"><%= link_to("All projects",projects_path) %></div>
</h4>
