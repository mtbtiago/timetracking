<h2>Entries for <%=@project.full_name%></h1>
<% if !@entries.empty? %>
  <ul>
    <% @entries.each do |e| %>
      <li>
        <%= "Date: #{e.date}. Hours: #{e.hours} Minutes: #{e.minutes}" %>
        <% if e.comments.present? %>
        <pre><%= "Optional comment: #{e.comments}"%></pre>
        <% end %>
      </li>
    <%end%>
  </ul>
  <h3>Total month: <%=@total_month_hours%></h3>
<%end%>
